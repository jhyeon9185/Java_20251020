package test;

public class Sample {

	public static void main(String[] args) {
		
		System.out.println("Hello, JAVA" );
		System.out.println("Hello, JAVA");
		
		// 아스키 코드, 유니코드 0X, 0B
		char var1 = 'A';
		char c1 = 97;
		
		char var2 = '가';
//		char c2 = 44032;
		char c2 = 0xAC63;
		
		
		System.out.println(var1);
		System.out.println(c1);
		
		System.out.println(var2);
		System.out.println(c2);
		
		float f1 = 10.5f;
		
		double d1 = 10.5;
		
		System.out.println(f1, d1);
	
		
	}

}
